            "image": "862327261051.dkr.ecr.ap-northeast-2.amazonaws.com/de-3-2/backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
